@{
    var culture = System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant();
}

<!doctype html>
<html lang="en" ng-app="gtokenApp" class="@ViewBag.body_classes">
<head>
    @*{% block head %}*@
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="csrf-token" content="{{ csrf_token() }}" />
    <meta property="fb:app_id" content="812688685436100" />
    @*{% include 'includes/favicon_meta.html' %}*@
    @Html.Partial("~/Views/Shared/includes/favicon_meta.cshtml")
    <title>GToken - @ViewBag.Title</title>
    <base href="/" />
    
    @*{% block open_graph_meta_tags %}*@
    @RenderSection("open_graph_meta_tags", false)
    <meta property="og:image" content="/static/images/gtoken_icon.png" />
    <meta property="og:site_name" content="GToken" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="Share GoPlay Token with your friends and loved ones, and empower them with the freedom to upgrade whichever games they like to play" />
    @*{% endblock %}
        {% endblock %}*@
    <link rel="stylesheet" href="/static/stylesheets/app.css" />
    <script src="/bower_components/modernizr/modernizr.js"></script>
    @RenderSection("head_css", false)
    @*{% block head_css %}
        {% endblock %}*@
    @RenderSection("page_stylesheets", false)
</head>

<body class="@ViewBag.body_classes">

    <div data-offcanvas="" class="off-canvas-wrap">
        <div class="inner-wrap">
            @*{% block header %}
                {% include 'includes/header.html' %}
                {% endblock %}*@
            @Html.Partial("~/Views/Shared/includes/header.cshtml")

            <section class="main-section">
                @*{% block content %}*@
                @RenderBody()
                @*{% endblock %}*@

                @*{% block footer %}*@
                @*{% include 'includes/footer.html' %}*@
                @Html.Partial("~/Views/Shared/includes/footer.cshtml")
                @*{% endblock %}*@
            </section>
        </div>
    </div>

    @RenderSection("foot_js_lib", false)
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/angular/angular.js"></script>
    <script src="/bower_components/angular-animate/angular-animate.js"></script>
    <script src="/bower_components/foundation/js/foundation.min.js"></script>
    <script src="/bower_components/zeroclipboard/dist/ZeroClipboard.js"></script>
    <script src="/static/libs/foundation-datetimepicker/foundation-datetimepicker.js"></script>
    @*<script src="/bower_components/moment/moment.js"></script>*@
    <script src="/bower_components/angucomplete-alt/angucomplete-alt.js"></script>
    <script src="/bower_components/ng-file-upload/ng-file-upload-shim.min.js"></script> <!-- for no html5 browsers support -->
    <script src="/bower_components/ng-file-upload/ng-file-upload.min.js"></script>
    <script src="/static/js/vendors/angular-page-enter-animate.js"></script>
    <script src="/static/js/app.js"></script>

    @RenderSection("foot_js", false)

    <!-- Google Analytic -->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga'); ga('create', 'UA-51518799-2', 'auto');
        ga('send', 'pageview');</script>
