            @if (Model.source != null && Model.source.Any())
            {
                <div class="table-wrap">
                    <table>
                        <thead>
                            <tr>
                                <th>From</th>
                                <th>To</th>
                                <th>Game</th>
                                <th>Number of active users</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var record in Model.source)
                            {
                                <tr class="@(record.name == "All games" ? "bold" : string.Empty)">
                                    <td>@record.fromDate</td>
                                    <td>@record.toDate</td>
                                    <td>@record.name</td>
                                    <td>@record.count</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            }

        </div>
    </div>


