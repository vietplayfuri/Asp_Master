@using Platform.Utility
@model GoPlay.Web.Areas.Admin.Models.AdminReportActiveUserModel
@{
    ViewBag.Title = "Active users report";
}
<link href="~/static/js/libs/foundation-datetimepicker/example/stylesheets/foundation-datepicker.css" rel="stylesheet" />
<div>

    <div class="row">
        <h1>Active users report</h1>
        <div class="content active">
            <form class="model-form" method="post">
                <div class="row">
                    <div class="row">
                        <div class="small-3 columns">
                            <label class="right inline " for="fromTime">
                                From *
                            </label>
                        </div>
                        <div class="small-9 columns">
                            <input type="datetime" value="@Model.fromTime" name="fromTime" id="fromTime">
                        </div>
                    </div>


                    <div class="row">
                        <div class="small-3 columns">
                            <label class="right inline " for="toTime">
                                To *
                            </label>
                        </div>
                        <div class="small-9 columns">
                            <input type="datetime" value="@Model.toTime" name="toTime" id="toTime">
                        </div>
                    </div>


                    <div class="row">
                        <div class="small-3 columns">
                            <label class="right inline " for="timezone">
                                Timezone
                            </label>
                        </div>
                        <div class="small-9 columns">
                            @Html.DropDownListFor(m => m.timezone, Helper.GetTimeZones().Select(x => new SelectListItem() { Text = x.DisplayName, Value = x.Id }))
                        </div>
                    </div>

                </div>
                <div class="small-6 columns"></div>
                <div class="row">
                    <div class="small-9 small-offset-3 columns">
                        <input class="button small" id="query" name="query" type="submit" value="Query">
                    </div>
                </div>
            </form>

