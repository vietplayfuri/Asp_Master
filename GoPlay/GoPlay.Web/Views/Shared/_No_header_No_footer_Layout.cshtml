    <script src="/bower_components/angular-cookies/angular-cookies.js"></script>
    <script src="/bower_components/angucomplete-alt/angucomplete-alt.js"></script>
    <script src="/bower_components/perfect-scrollbar/js/min/perfect-scrollbar.jquery.min.js"></script>
    <script src="/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
    <script type="text/javascript" src="/static/js/routing.js"></script>
    @RenderSection("foot_js_lib", false)

    <script src="/static/js/app.js?rnd=20042015"></script>
    <script src="/static/js/controllers.js?rnd=20042015"></script>
    <script type="text/javascript" src="/static/js/wow.min.js"></script>
    <!--script src="/static/js/chat.js"></script-->
    <script src="/static/js/friend.js"></script>
    <script>
        new WOW().init();
        $(function () {
            $('.perfectScroll').perfectScrollbar();
            $(window).on('resize.stickyFooter', function () {
                var footerHeight = $('footer').outerHeight();
                var stickFooterPush = $('.push').height(footerHeight);

                $('.wrapper').css({ 'marginBottom': '-' + footerHeight + 'px' });
            });
            $(window).trigger('resize.stickyFooter');
        });
    </script>
    @RenderSection("page_foot_js", false)


