@{
    var culture = System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant();
}

<!DOCTYPE html>
<html lang="en" ng-app="gtokenApp" class="html_classes">
<head>
    <meta charset="UTF-8" />
    @Html.Partial("~/Views/Shared/Includes/favicon_meta.cshtml")
    <meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=0" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no" />
    @Html.AntiForgeryToken()
    <meta property="fb:app_id" content="812688685436100" />

    <title>GoPlay - @ViewBag.Title</title>

    <base href="@Url.Action("Index", "Home")" />
    <meta property="og:image" content="@String.Format("{0},{1}",@Request.Url.Scheme,"/static/images/gtoken_icon.png")" />
    <meta property="og:site_name" content="GoPlay" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="Share GoPlay with your friends and loved ones, and empower them with the freedom to upgrade whichever games they like to play" />
    @RenderSection("open_graph_meta_tags", false)
    <link href='/static/fonts/ubuntu/ubuntu.css' rel='stylesheet' type='text/css' charset="utf-8">
    <link rel="stylesheet" href="/static/stylesheets/main.css?rnd=20042015" />
    <link rel="stylesheet" href="/bower_components/perfect-scrollbar/css/perfect-scrollbar.min.css" />
    @RenderSection("page_stylesheets", false)
    <link rel="stylesheet" type="text/css" href="/bower_components/slick.js/slick/slick.css" />
    @RenderSection("lib_stylesheets", false)
    @if (culture != "en-us")
    {
        string url = "static/stylesheets/main_" + culture.Substring(0, 2) + ".css?rnd=20042015";
        <link rel="stylesheet" href="/static/stylesheets/main_@culture" />
    }

    @RenderSection("head_js_lib", false)
    <script src="/bower_components/modernizr/modernizr.js"></script>
    <script src="/bower_components/moment/moment.js"></script>

</head>
<body class="body_classes">
    @*{% block flashed_messages %}
        {% with messages = get_flashed_messages(with_categories=true) %}
        {% if messages %}
        {% for category, message in messages %}
        <div data-alert class="alert-box radius {{ category }}" tabindex="0" aria-live="assertive" role="dialogalert">
            {{ message|safe }}
            <a href="#" class="close">&times;</a>
        </div>
        {% endfor %}
        {% endif %}
        {% endwith %}
        {% endblock %}*@

    <div class="wrapper">
        @RenderBody()
    </div>
    <div class="push"></div>
    
    <div class="body-filter hide">
    </div>


    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/foundation/js/foundation.min.js"></script>
